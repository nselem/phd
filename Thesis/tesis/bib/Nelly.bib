}

@article{barglow_activity-based_2007,
	title = {Activity-based protein profiling for the functional annotation of enzymes},
	volume = {4},
	copyright = {© 2007 Nature Publishing Group},
	issn = {1548-7091},
	url = {http://www.nature.com/nmeth/journal/v4/n10/full/nmeth1092.html},
	doi = {10.1038/nmeth1092},
	abstract = {Activity-based protein profiling (ABPP), the use of active site-directed chemical probes to monitor enzyme function in complex biological systems, is emerging as a powerful post-genomic technology. ABPP probes have been developed for several enzyme classes and have been used to inventory enzyme activities en masse for a range of (patho) physiological processes. By presenting specific examples, we show here that ABPP provides researchers with a distinctive set of chemical tools to embark on the assignment of functions to many of the uncharacterized enzymes that populate eukaryotic and prokaryotic proteomes.},
	language = {en},
	number = {10},
	urldate = {2017-02-08},
	journal = {Nature Methods},
