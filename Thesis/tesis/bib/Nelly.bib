	pages = {2966--2971},
	file = {Full Text PDF:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/WKJCXSEN/Huang et al. - 2012 - Enzyme functional evolution through improved catal.pdf:application/pdf;Full Text PDF:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/TW2T2T63/Huang et al. - 2012 - Enzyme functional evolution through improved catal.pdf:application/pdf;Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/HWU4ESSQ/2966.html:text/html;Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/ZEV33938/2966.html:text/html}
