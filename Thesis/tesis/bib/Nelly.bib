	volume = {26},
	issn = {1096-987X},
	shorttitle = {{GROMACS}},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/jcc.20291/abstract},
	doi = {10.1002/jcc.20291},
	abstract = {This article describes the software suite GROMACS (Groningen MAchine for Chemical Simulation) that was developed at the University of Groningen, The Netherlands, in the early 1990s. The software, written in ANSI C, originates from a parallel hardware project, and is well suited for parallelization on processor clusters. By careful optimization of neighbor searching and of inner loop performance, GROMACS is a very fast program for molecular dynamics simulation. It does not have a force field of its own, but is compatible with GROMOS, OPLS, AMBER, and ENCAD force fields. In addition, it can handle polarizable shell models and flexible constraints. The program is versatile, as force routines can be added by the user, tabulated functions can be specified, and analyses can be easily customized. Nonequilibrium dynamics and free energy determinations are incorporated. Interfaces with popular quantum-chemical packages (MOPAC, GAMES-UK, GAUSSIAN) are provided to perform mixed MM/QM simulations. The package includes about 100 utility and analysis programs. GROMACS is in the public domain and distributed (with source code and documentation) under the GNU General Public License. It is maintained by a group of developers from the Universities of Groningen, Uppsala, and Stockholm, and the Max Planck Institute for Polymer Research in Mainz. Its Web site is http://www.gromacs.org. © 2005 Wiley Periodicals, Inc. J Comput Chem 26: 1701–1718, 2005},
	language = {en},
	number = {16},
	urldate = {2017-02-09},
	journal = {Journal of Computational Chemistry},
	author = {Van Der Spoel, David and Lindahl, Erik and Hess, Berk and Groenhof, Gerrit and Mark, Alan E. and Berendsen, Herman J. C.},
	month = dec,
	year = {2005},
	keywords = {free energy computation, GROMACS, molecular dynamics, molecular simulation software, parallel computation},
	pages = {1701--1718},
	file = {Full Text PDF:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/E5H82A9S/Van Der Spoel et al. - 2005 - GROMACS Fast, flexible, and free.pdf:application/pdf;Full Text PDF:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/GUCNP59D/Van Der Spoel et al. - 2005 - GROMACS Fast, flexible, and free.pdf:application/pdf;Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/WD436A7Q/abstract.html:text/html;Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/WXHATRSF/abstract.html:text/html}
}

@article{treangen_horizontal_2011,
	title = {Horizontal {Transfer}, {Not} {Duplication}, {Drives} the {Expansion} of {Protein} {Families} in {Prokaryotes}},
	volume = {7},
	issn = {1553-7404},
	url = {http://journals.plos.org/plosgenetics/article?id=10.1371/journal.pgen.1001284},
	doi = {10.1371/journal.pgen.1001284},
	abstract = {Author Summary Prokaryotes can be found in the most diverse and severe ecological niches of the planet. Their rapid adaptation is, in part, the result of the ability to acquire genetic information horizontally. This means that prokaryotes utilize two major paths to expand their repertoire of protein families: they can duplicate a pre-existing gene or acquire it by horizontal transfer. In this study, we track family expansions among closely related strains of prokaryotic species. We find that the majority of gene expansions arrive via transfer not via duplication. Additionally, we find that duplicate genes tend be more transient and evolve slower than transferred ones, highlighting different roles with respect to adaptation and evolution. These results suggest that prevailing theories aimed at understanding the evolution of biological systems grounded on gene duplication might be poorly fit to explain the evolution of prokaryotic systems, which include the vast majority of life's biochemical diversity.},
	number = {1},
	urldate = {2017-02-09},
	journal = {PLOS Genetics},
	author = {Treangen, Todd J. and Rocha, Eduardo P. C.},
	month = jan,
	year = {2011},
	keywords = {Evolutionary genetics, Gene duplication, Genome evolution, Genomic databases, Helicobacter, Horizontal gene transfer, Neisseria, Prokaryotic cells},
	pages = {e1001284},
	file = {Full Text PDF:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/HNMUUMQI/Treangen and Rocha - 2011 - Horizontal Transfer, Not Duplication, Drives the E.pdf:application/pdf;Full Text PDF:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/ZU5TSZ3Q/Treangen and Rocha - 2011 - Horizontal Transfer, Not Duplication, Drives the E.pdf:application/pdf;Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/IZ33N9CU/article.html:text/html;Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/56FDR58R/article.html:text/html}
