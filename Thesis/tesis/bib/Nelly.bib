@article{yang_molecular_2013,
	title = {Molecular {Networking} as a {Dereplication} {Strategy}},
	volume = {76},
	issn = {0163-3864},
	url = {http://dx.doi.org/10.1021/np400413s},
	doi = {10.1021/np400413s},
	abstract = {A major goal in natural product discovery programs is to rapidly dereplicate known entities from complex biological extracts. We demonstrate here that molecular networking, an approach that organizes MS/MS data based on chemical similarity, is a powerful complement to traditional dereplication strategies. Successful dereplication with molecular networks requires MS/MS spectra of the natural product mixture along with MS/MS spectra of known standards, synthetic compounds, or well-characterized organisms, preferably organized into robust databases. This approach can accommodate different ionization platforms, enabling cross correlations of MS/MS data from ambient ionization, direct infusion, and LC-based methods. Molecular networking not only dereplicates known molecules from complex mixtures, it also captures related analogues, a challenge for many other dereplication strategies. To illustrate its utility as a dereplication tool, we apply mass spectrometry-based molecular networking to a diverse array of marine and terrestrial microbial samples, illustrating the dereplication of 58 molecules including analogues.},
	number = {9},
	urldate = {2017-02-09},
	journal = {Journal of Natural Products},
	author = {Yang, Jane Y. and Sanchez, Laura M. and Rath, Christopher M. and Liu, Xueting and Boudreau, Paul D. and Bruns, Nicole and Glukhov, Evgenia and Wodtke, Anne and de Felicio, Rafael and Fenner, Amanda and Wong, Weng Ruh and Linington, Roger G. and Zhang, Lixin and Debonsi, Hosana M. and Gerwick, William H. and Dorrestein, Pieter C.},
	month = sep,
	year = {2013},
	pages = {1686--1699},
	file = {ACS Full Text PDF w/ Links:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/CTMMA6FK/Yang et al. - 2013 - Molecular Networking as a Dereplication Strategy.pdf:application/pdf;ACS Full Text Snapshot:/home/nelly/.mozilla/firefox/8e2ipeap.default/zotero/storage/9D5B5JJT/np400413s.html:text/html}
}

